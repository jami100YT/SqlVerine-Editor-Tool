{
  "manifest_version": 3,
  "name": "Sqlverine Editor Tool",
  "version": "1.0",
  "description": "Eine Erweiterung des Sqlverine.org SQL Editors.",
  "permissions": [
    "activeTab"
  ],
  "web_accessible_resources": [
    {
      "resources": ["popup.html", "popup.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["*://editor.sqlverine.org/*"],
      "js": ["index.js"]
    }
  ]
}
