{
  "manifest_version": 3,
  "name": "Sqlverine Editor Tool",
  "version": "1.0",
  "description": "Eine Erweiterung f√ºr den Sqlverine.org SQL Editor.",
  "permissions": [
    "activeTab"
  ],
  "web_accessible_resources": [
    {
      "resources": ["codeWindow.html", "codeWindow.js"],
      "matches": ["*://editor.sqlverine.org/*"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["*://editor.sqlverine.org/*"],
      "js": ["index.js"]
    }
  ]
}
